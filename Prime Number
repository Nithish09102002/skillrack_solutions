import java.util.*;
public class Hello {

    public static void main(String[] args) {
	Scanner input=new Scanner(System.in);
	int n=input.nextInt();
	boolean flag=false;
	if(n==2)System.out.print("YES");
	else if(n==3)System.out.print("YES");
	else {
	for(int i=2;i<=n/2;i++){
	    if(n%i==0)flag=true;
	    else flag=false;
	    
	    if(flag){
	        System.out.print("NO");
	        break;
	    }

	}
	if(!flag)System.out.print("YES");
	}

	}
}
