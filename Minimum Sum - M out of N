import java.util.*;
public class Hello {

    public static void main(String[] args) {
	Scanner input=new Scanner(System.in);
	int n=input.nextInt();
	int m=input.nextInt();
	int min=0;
	int[] arr=new int[n];
	for(int i=0;i<n;i++){
	    arr[i]=input.nextInt();
	}
	Arrays.sort(arr);
	for(int i=0;i<m;i++){
	    min +=arr[i];
	}
	System.out.print(min);

	}
}
