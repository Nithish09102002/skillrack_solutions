import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		String[] str1=input.nextLine().split(" ");
		String[] str2=input.nextLine().split(" ");
		int alenc=Integer.parseInt(str1[0])/5;
		int timc=Integer.parseInt(str1[1])/5;
		
		int alen=alenc/Integer.parseInt(str2[0]);
		int tim=timc/Integer.parseInt(str2[1]);
		System.out.printf("%.2f",(float)Math.abs(alen-tim));

	}
}
