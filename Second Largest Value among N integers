import java.util.*;
public class Hello {

    public static void main(String[] args) {
	Scanner input=new Scanner(System.in);
	int n=input.nextInt();
    int[] arr=new int[n];
    int i=0;
	while(n-- >0){
	    arr[i++]=input.nextInt();
	}
	Arrays.sort(arr);
	System.out.print(arr[arr.length-2]);

	}
}
