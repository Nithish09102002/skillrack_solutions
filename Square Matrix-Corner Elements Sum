import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		input.nextLine();
		int sum=0;
		for(int i=0;i<n;i++){
		 String[] str=input.nextLine().split("\\s");
		   if(i==0 || i==n-1){
  sum =sum + Integer.parseInt(str[0])+Integer.parseInt(str[n-1]);
         
		   }
		}
		System.out.print(sum);
		
        
        
    }
}
