import java.util.*;
public class Hello {

    public static void main(String[] args) {
	Scanner input=new Scanner(System.in);
	int n=input.nextInt();
	for(int i=n+1;i>0;i++){
	    boolean flag=true;
	    for(int j=4;j<n;j++){
	        if(i%j==0){
	            flag=false;
	            break;
	            
	        }
	    }
	        if(flag){
	            System.out.print(i);
	            break;
	        }
	    }
	}

	
}
